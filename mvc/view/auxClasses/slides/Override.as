package slides {	import com.greensock.*;	import com.greensock.easing.*;		import flash.display.DisplayObject;	import flash.display.MovieClip;		public class Override extends Slide {		public var textMcs:Array;						public function Override() {			super();		}				override protected function init():void {			this.visible = false;			this.alpha = 0;			textMcs = [];			for (var i:int = 1; i <= 5; i++) {				textMcs.push( this.getChildByName("text" + i) );			}		}						override public function animateIn():TimelineLite {			TweenMax.allTo(textMcs, 0, {alpha:0, scaleX:.5, scaleY:.5, rotationX:0});			var tl:TimelineLite = new TimelineLite();			tl.insert( TweenMax.to(this, 0, {autoAlpha:1, immediateRender:false}) );			tl.appendMultiple( TweenMax.allTo(textMcs, .6, {alpha:1, scaleX:1, scaleY:1}, 0.3) );			return tl;		}				override public function animateOut():TimelineLite {			var tl:TimelineLite = new TimelineLite();			tl.appendMultiple( TweenMax.allTo(textMcs, .5, {alpha:0, scaleX:8, scaleY:8, rotationX:160, y:"50"}, 0.1) );			tl.append( TweenMax.to(this, 0.01, {autoAlpha:0, immediateRender:false}) );			return tl;		}					}}